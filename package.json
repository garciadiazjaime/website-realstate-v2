{
  "name": "propiedades",
  "description": "propiedades",
  "version": "0.0.1",
  "scripts": {
    "dev": "sapper dev",
    "build": "sapper build --legacy",
    "export": "NODE_ENV=production API_URL=http://api.mintitmedia.com sapper export --legacy",
    "start": "node __sapper__/build",
    "google:clean": "gsutil -m rm -r gs://realstate2.mintitmedia.com/*",
    "google:upload": "gsutil -m cp -r ./__sapper__/export/. gs://realstate2.mintitmedia.com/",
    "deploy": "npm run build && npm run google:clean && npm run google:upload"
  },
  "dependencies": {
    "compression": "^1.7.1",
    "polka": "next",
    "sirv": "^1.0.0"
  },
  "devDependencies": {
    "sapper": "^0.28.0",
    "svelte": "^3.17.3",
    "@babel/core": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/runtime": "^7.0.0",
    "@rollup/plugin-babel": "^5.0.0",
    "@rollup/plugin-commonjs": "^14.0.0",
    "@rollup/plugin-node-resolve": "^8.0.0",
    "@rollup/plugin-replace": "^2.2.0",
    "@rollup/plugin-url": "^5.0.0",
    "rollup": "^2.3.4",
    "rollup-plugin-svelte": "^7.0.0",
    "rollup-plugin-terser": "^7.0.0"
  }
}
